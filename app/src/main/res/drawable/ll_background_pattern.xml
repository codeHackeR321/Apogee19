<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="top|right"
        android:right="90dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="120dp"
                android:height="90dp" />
            <corners
                android:bottomLeftRadius="20dp"
                android:bottomRightRadius="20dp" />
            <solid android:color="@color/wht02" />
        </shape>
    </item>

    <item
        android:gravity="top|right"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="150dp"
                android:height="36dp" />
            <corners android:bottomLeftRadius="20dp" />
            <solid android:color="@color/wht02" />
        </shape>
    </item>

    <item
        android:gravity="left|top"
        android:left="0dp"
        android:top="320dp">
        <shape android:shape="rectangle">
            <size
                android:width="132dp"
                android:height="132dp" />
            <corners
                android:bottomRightRadius="20dp"
                android:topRightRadius="20dp" />
            <solid android:color="@color/wht02" />
        </shape>
    </item>

    <item
        android:gravity="left|top"
        android:left="0dp"
        android:top="260dp">
        <shape android:shape="rectangle">
            <size
                android:width="50dp"
                android:height="130dp" />
            <corners
                android:bottomRightRadius="20dp"
                android:topRightRadius="20dp" />
            <solid android:color="@color/wht02" />
        </shape>
    </item>
</layer-list>